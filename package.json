  "repository": {
    "type": "git",
    "url": "git+https://github.com/memcallen/galvanize.git"
  },
  "bugs": {
    "url": "https://github.com/memcallen/galvanize/issues"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "babel": "^6.23.0",
    "babel-loader": "^8.2.4",
    "file-loader": "^6.2.0",
    "typescript": "^4.6.3",
    "webpack": "^5.71.0",
    "webpack-cli": "^4.9.2",
    "core-js": "^3.21.1"
  }
